{
  "name": "sg-server",
  "version": "1.0.0",
  "description": "",
  "engines": {
    "node": "15.12.0",
    "npm": "7.6.3"
  },
  "scripts": {
    "start": "nodemon ./src/server.js -e js, graphql --exec babel-node",
    "serve": "node build/server.js",
    "build:server": "babel src/ --out-dir build",
    "syncGraphql": "rsync -a --prune-empty-dirs --include 'src/types/*' --include '*.graphql' 'src/types' 'build'",
    "build": "concurrently \"npm run build:server\" \"npm run syncGraphql\""
  },
  "author": "Diego Cabanillas",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.14.3",
    "@babel/node": "^7.14.2",
    "@babel/plugin-transform-runtime": "^7.14.3",
    "@babel/preset-env": "^7.14.2",
    "@babel/register": "^7.13.16",
    "@babel/runtime": "^7.14.0",
    "apollo-server": "^2.25.0",
    "aws-sdk": "^2.917.0",
    "bcrypt": "^5.0.1",
    "concurrently": "^6.2.0",
    "dataloader": "^2.0.0",
    "dotenv": "^10.0.0",
    "googleapis": "^74.2.0",
    "graphql": "^15.5.0",
    "graphql-tools": "^7.0.5",
    "jsonwebtoken": "^8.5.1",
    "nodemailer": "^6.6.1",
    "nodemailer-express-handlebars": "^4.0.0",
    "nodemon": "^2.0.7",
    "pg": "^8.6.0",
    "pg-hstore": "^2.3.3",
    "sequelize": "^6.6.2",
    "sequelize-cli": "^6.2.0",
    "shortid": "^2.2.16"
  }
}
